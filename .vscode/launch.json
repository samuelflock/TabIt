{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Using MP3",
            "type": "debugpy",
            "request": "launch",
            "program": "TabIt",
            "console": "integratedTerminal",
            "args": ["../music/File Name.mp3", "-v"]
        },
        {
            "name": "Python Debugger: Using YouTube",
            "type": "debugpy",
            "request": "launch",
            "program": "TabIt",
            "console": "integratedTerminal",
            "args": ["https://www.youtube.com/watch?app=desktop&v=WOEJbo4XZFU", "-v"]
        }
    ]
}